{
  "name": "casazium-license",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node src/app.js",
    "start": "node dist/index.obfuscated.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "node esbuild.config.mjs",
    "obfuscate": "javascript-obfuscator dist/index.js --config obfuscator.config.json --output dist/index.obfuscated.js",
    "build:prod": "npm run build && npm run obfuscate",
    "docker:build": "docker build -t casazium-license .",
    "docker:run": "docker-compose up",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "check:docker": "node scripts/check-docker-context.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "sqlite3": "^5.1.7",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "coveralls": "^3.1.1",
    "esbuild": "^0.20.0",
    "execa": "^9.6.0",
    "javascript-obfuscator": "^4.1.1",
    "nock": "^14.0.5",
    "node-fetch": "^3.3.2",
    "vitest": "^3.1.4"
  }
}